{
  "name": "rock-clicker",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "node -e \"import('./lib/supabase/test-database.js').then(module => module.default()).then(console.log)\"",
    "test:user": "node scripts/test-user-creation.js",
    "fix:rls": "npx ts-node scripts/fix-supabase-rls.ts",
    "fix:rls:simple": "node scripts/fix-rls-simple.js",
    "deploy:ios": "npx eas build --platform ios --profile production && npx eas submit --platform ios",
    "deploy:android": "npx eas build --platform android --profile production && npx eas submit --platform android",
    "deploy": "npx eas build --platform all --profile production && npx eas submit --platform all"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/stack": "^7.2.3",
    "@supabase/supabase-js": "^2.49.3",
    "dotenv": "^16.4.7",
    "expo": "^52.0.16",
    "expo-av": "^15.0.2",
    "expo-clipboard": "^7.0.1",
    "expo-constants": "~17.0.2",
    "expo-dev-client": "^5.0.15",
    "expo-haptics": "^14.0.1",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.2",
    "expo-splash-screen": "~0.29.7",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-pager-view": "^6.7.0",
    "react-native-paper": "^5.13.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "^15.11.2",
    "react-native-tab-view": "^4.0.6",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.6",
    "react-native-webp-format": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
